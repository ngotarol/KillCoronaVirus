@model IEnumerable<FCE_KillCoronaVirus.Models.PresentacionFarmaco>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Pages/_ViewStart.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-area="Admin" asp-controller="PresentacionFarmaco" asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomPresentacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomPresentacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodPresentacion">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodPresentacion">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodPresentacion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
